      <AutoGen>True</AutoGen>
      <DependentUpon>SchemaNames.tt</DependentUpon>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Update="SchemaNames.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
